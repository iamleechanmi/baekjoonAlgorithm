package com.step07;

import java.util.Scanner;

public class Ex09 {
	
	public static void main(String[] args) {
		
		//크로아티아 알파벳
		Scanner sc = new Scanner(System.in);
		String line = sc.nextLine();
		sc.close();
		
		int cnt = 0;
		
		for (int i=0; i<line.length(); i++) {
			
			char alpha = line.charAt(i);
			
			if (i < line.length() - 1) {
				
				if (alpha == 'c') {
					if (line.charAt(i+1) == '=' || line.charAt(i+1) == '-') {
						i++;
					}
				}
				
				if (alpha == 'l') {
					if (line.charAt(i+1) == 'j') {
						i++;
					}
				}
				
				if (alpha == 'n') {
					if (line.charAt(i+1) == 'j') {
						i++;
					}
				}
				
				if (alpha == 's') {
					if (line.charAt(i+1) == '=') {
						i++;
					}
				}
				
				if (alpha == 'z') {
					if (line.charAt(i+1) == '=') {
						i++;
					}
				}
				
			} else if (i < line.length() - 2) {
				
				if (alpha == 'd') {
					if (line.charAt(i+1) == '-') {
						i++;
					} else if (line.charAt(i+1) == 'z' && line.charAt(i+2) == '=') {
						i = i + 2;
					}
				}
			}
			cnt++;
		}
		
		System.out.println(cnt);
	}
}
