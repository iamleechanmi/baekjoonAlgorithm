package step03;

public class ex02 {

	public static void main(String[] args) {

		Scanner scan = new Scanner(System.in);

		int number = scan.nextInt();

		for (int i=0; i<number; i++){
			int number1 = scan.nextInt();
			int number2 = scan.nextInt();

			System.out.println(number1 + number2);
	}
}
