package com.step16;

import java.util.Scanner;

public class Ex02 {

	public static void main(String[] args) {
		
		//약수
		Scanner sc = new Scanner(System.in);
		int cnt = sc.nextInt();
		int[] divisior = new int[cnt];

		int number = 1;
		for (int i=0; i<cnt; i++) {
			divisior[i] = sc.nextInt();
			number *= divisior[i];
		}
		System.out.println(number);
	}
}
