package step05;

import java.util.Scanner;

public class ex03 {

	public static void main(String[] args) {
		
		//숫자의 개수
		Scanner sc = new Scanner(System.in);
		int[] number = new int[3];
		int result = 1;
		
		for (int i=0; i<3; i++) {
			number[i] = sc.nextInt();
			result = result * number[i];
		}
		sc.close();
		
		for (int i=0; i<result.size; i++) {
			
		}
	}
}
