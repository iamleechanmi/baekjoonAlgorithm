package com.step05;

import java.util.Scanner;

public class Ex04 {
	
	public static void main(String[] args) {
		
		//나머지
		Scanner sc = new Scanner(System.in);
		int[] nums = new int[10];
		int cnt = 1;
		
		for (int i=0; i<10; i++) {
			int tmp = 0;
			nums[i] = sc.nextInt() % 42;
			
			if (i == 0) {
				tmp = nums[i];
			} 
			
			if (tmp != nums[i]) {
				cnt++;
				tmp = nums[i];
			}
		}
		sc.close();
		
		System.out.println(cnt);
	}
}
