package step05;

import java.util.Scanner;

public class Ex04 {

	public static void main(String[] args) {
		
		//나머지
		Scanner sc = new Scanner(System.in);
		int[] number = new int[10];
		
		int cnt = 1;
		int differ = sc.nextInt() % 42;
		
		for (int i=1; i<10; i++) {
			number[i] = sc.nextInt() % 42;

			if (differ != number[i]) {
				differ = number[i];
				cnt++;
			}
		}
		System.out.println(cnt);
	}
}
