package com.step07;

import java.util.Scanner;

public class Ex02 {

	public static void main(String[] args) {
		
		//숫자의 합
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int sum = 0;
		
		String line = sc.nextLine();
		for (int i=0; i<n; i++) {
			System.out.println(line.charAt(i));
		}
	}
}
