package com.step05;

import java.util.Scanner;

public class Ex06 {
	
	public static void main(String[] args) {
		
		//OX퀴즈
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
//		String[] result = new String[n]; 
		
		int score = 0; //최종 점수 더할 변수
		int tmp = 0; //임시로 점수 저장할 변수
		int cnt = 1; //루프 카운트 변수
		
		while (true) {
			
			if (cnt == n) {
				break;
			}
			
			String str = sc.next();
			
			if (str.equals("O")) {
				tmp += 1;
			} else if (str.equals("X")) {
				tmp = 0;
			} else {
				cnt++;
				continue;
			}
			
		}
		
		sc.close();
	}

}
