package step05;

import java.util.Scanner;

public class ex02 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int[] number = new int[9];
		number[0] = sc.nextInt();
		
		int line = 0;
		int max = number[0];
		
		for (int i=1; i<9; i++) {
			number[i] = sc.nextInt();
			
			if (number[i] > max) {
				max = number[i];
			}
		}
		
		sc.close();
	}
}
