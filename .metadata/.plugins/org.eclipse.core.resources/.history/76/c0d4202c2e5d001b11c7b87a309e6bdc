package com.step07;

import java.util.Scanner;

public class Ex04 {

	public static void main(String[] args) {
		
		//문자열 반복
		Scanner sc = new Scanner(System.in);
		int cnt = sc.nextInt();
		
		for (int i=0; i<cnt; i++) {
			int repeat = sc.nextInt();
			String str = sc.next();
			System.out.print(str);
		}
	}
}
