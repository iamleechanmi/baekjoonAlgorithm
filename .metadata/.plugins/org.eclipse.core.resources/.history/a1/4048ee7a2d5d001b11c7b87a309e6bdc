package com.step16;

import java.util.Scanner;

public class Ex09 {
	
	public static void main(String[] args) {
		
		//다리 놓기
		Scanner sc = new Scanner(System.in);
		int cnt = sc.nextInt();
		
		for (int i=0; i<cnt; i++) {
			int number1 = sc.nextInt();
			int number2 = sc.nextInt();
			
			int result = 0;
			for (int j=0; j<number2; j++) {
				result = number1 * 
			}
			
		}
		sc.close();
	}
}
